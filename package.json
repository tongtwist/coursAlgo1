{
  "name": "coursalgo",
  "version": "0.1.0",
  "description": "",
  "main": "publics/scipts/index.js",
  "scripts": {
    "test": "mocha --require ts-node/register --require mochawesome/register --reporter mochawesome test/**/*.spec.ts",
    "build": "run-s build:doc build:ts",
    "build:ts": "tsc",
    "build:doc": "rimraf ./doc/* && typedoc",
    "watch": "run-p watch:*",
    "watch:doc": "rimraf ./doc/* && typedoc --watch",
    "watch:ts": "tsc --watch",
    "clean": "rimraf dist/",
    "start:dev": "npm run clean && npm run build && run-p watch start",
    "start": "nodemon public/scripts/index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.4.4"
  },
  "nodemonConfig": {
    "delay": 500
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "type": "module"
}
